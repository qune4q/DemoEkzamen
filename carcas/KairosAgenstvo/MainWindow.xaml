<Window x:Class="KairosAgenstvo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KairosAgenstvo"
        mc:Ignorable="d"
        Title="TurAgenstvo" MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/Resources/logo.png" HorizontalAlignment="Left" />
        <TextBlock Text="TurAgenstvo" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"> </TextBlock>
        <Grid Background="#C10C06" Panel.ZIndex="-3"></Grid>
        <Grid Grid.Row="2" Background="#C10C06"></Grid>
        <Frame Grid.Row="1" Name="MainFrame"></Frame>
    </Grid>
</Window>
